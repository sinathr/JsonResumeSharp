<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Description>A library for creating and manipulating Json Resume object it in C#</Description>
    <PackageProjectUrl>https://github.com/sinathr/JsonResumeSharp</PackageProjectUrl>
    <RepositoryType>public</RepositoryType>
    <PackageTags>Json , Resume , JsonResume</PackageTags>
    <PackageReleaseNotes>Beta Versian</PackageReleaseNotes>
    <RepositoryUrl>https://github.com/sinathr/JsonResumeSharp</RepositoryUrl>
    <Version>1.0.0</Version>
    <Authors>sinathr</Authors>
    <NeutralLanguage></NeutralLanguage>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageLicenseUrl>https://github.com/sinathr/JsonResumeSharp/blob/master/LICENSE</PackageLicenseUrl>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="12.0.3" />
  </ItemGroup>

</Project>
